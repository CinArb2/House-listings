{
  "compilerOptions": {
    "target": "ES2016",
    "module": "CommonJS",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "baseUrl": ".",
    "noImplicitAny": true,
    "noUnusedParameters": true,
    "resolveJsonModule": true
  },
  "include": ["body-request.json"],
  "exclude": ["node_modules"]
}

